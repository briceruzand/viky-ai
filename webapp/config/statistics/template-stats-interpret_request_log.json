{
  "version": 6,
  "index_patterns": "stats-interpret_request_log-*",
  "settings": {
    "number_of_shards": 2
  },
  "aliases": {
    "search-stats-interpret_request_log": {}
  },
  "mappings": {
    "dynamic_templates":
    [
      {
        "context_properties":
        {
          "mapping":
          {
            "type": "keyword"
          },
          "path_match": "context.*"
        }
      }
    ],
    "properties": {
      "timestamp": {
        "type": "date"
      },
      "agent_id": {
        "type": "keyword"
      },
      "agent_slug": {
        "type": "keyword"
      },
      "language": {
        "type": "keyword"
      },
      "spellchecking": {
        "type": "keyword"
      },
      "now": {
        "type": "date"
      },
      "owner_id": {
        "type": "keyword"
      },
      "sentence": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword",
            "ignore_above": 256
          }
        }
      },
      "status": {
        "type": "integer"
      },
      "fluentbit_time": {
        "type": "date",
        "index": false,
        "store": false
      },
      "body": {
        "properties": {
          "interpretations": {
            "properties": {
              "id": {
                "type": "keyword"
              },
              "package": {
                "type": "keyword"
              },
              "score": {
                "type": "float"
              },
              "slug": {
                "type": "keyword"
              },
              "solution": {
                "type": "object",
                "enabled": false
              },
              "explanation": {
                "type": "object",
                "enabled": false
              }
            }
          },
          "errors": {
            "type": "text"
          }
        }
      }
    }
  }
}
